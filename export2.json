{
    "export": "node-sequelize6",
    "zip": false,
    "dir": "build",
    "params": {
        "language": "english",
        "namingStrategy": "as-is",
        "logToConsole": false,
        "logFile": "",
        "backupExistingFile": true,
        "useTabs": false,
        "indentation": 4,
        "eolDelimeter": "win",
        "addGeneratorInfoAsComment": true,
        "filename": "%entity%.%extension%",
        "skipPluralNameChecking": false,
        "useLoggedStorage": false,
        "sortTablesAndViews": true,
        "exportOnlyTableCategorized": "",
        "enhanceManyToManyDetection": true,
        "skipManyToManyTables": true,
        "stripMultipleUnderscores": false,
        "asIsUserDatatypePrefix": "",
        "commonTableProp": "prop.json",
        "useSemicolon": true,
        "generateAssociationMethod": true,
        "generateForeignKeysFields": true,
        "injectExtendFunction": true
    }
}